my_list= []

#append elements to my_list

my_list.append(10)
my_list.append(20)
my_list.append(30)
my_list.append(40)

print("my_list:", my_list)

#Insert 15 at the second position of the list
my_list.insert(1, 15)

print("my_list:", my_list)

list2= [50, 60, 70]
my_list.extend(list2)

print("my_list:", my_list)
